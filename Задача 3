k = 0 #счётчик
s = '0123456789' #набор вариантов для каждой позиции
for x1 in s: #перебор вариантов для каждой позиции
    for x2 in s:
        for x3 in s:
            for x4 in s:
                l = x1 + x2 + x3 + x4 #образование цельного номера
                if l.count('5') == 1 and l.count('9') == 0: #проверка условия
                    k += 1 #счётчик
print(k) #вывод количества
