k = 0 #счётчик количества
s = '0123456789' 
for x1 in s: #перебор вариантов для первой цифры номера
    for x2 in s: #перебор вариантов для второй цифры номера 
        for x3 in s: #перебор вариантов для третьей цифры номера
            for x4 in s: #перебор вариантов для четвёртой цифры номера
                for x5 in s: #перебор вариантов для пятой цифры номера
                    for x6 in s: #перебор вариантов для шестой цифры номера
                        n = x1 + x2 + x3 + x4 + x5 + x6 
                        if n.count('7')<=1 and n[5] == '4': #проверка условия
                            k += 1 #счетчик
print(k)
